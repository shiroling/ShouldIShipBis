<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompareActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintEnd_toStartOf="@+id/linearLayout"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_poids1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_poids" />

                <TextView
                    android:id="@+id/compare_poids1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valpoids1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_distance1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_distance" />

                <TextView
                    android:id="@+id/compare_distance1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valdistance1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_carbone1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_carbone" />

                <TextView
                    android:id="@+id/compare_carbone1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valcarbone1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_date1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_date" />

                <TextView
                    android:id="@+id/compare_date1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valdate1" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/linearLayout2">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_poids2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_poids" />

                <TextView
                    android:id="@+id/compare_poids2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valpoids2" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_distance2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_distance" />

                <TextView
                    android:id="@+id/compare_distance2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valdistance2" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_carbone2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_carbone" />

                <TextView
                    android:id="@+id/compare_carbone2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valcarbone2" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compare_label_date2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/label_date" />

                <TextView
                    android:id="@+id/compare_date2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="valdate2" />

            </LinearLayout>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>